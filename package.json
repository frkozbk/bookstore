{
  "name": "bookstore",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "concurrently": "^4.1.0",
    "json-server": "^0.14.2",
    "lodash": "^4.17.11",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^4.4.0",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "api-install": "npm install --prefix api",
    "full-install": "concurrently \"npm run api-install\" \"npm install\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "api": "npm run jsonserver --prefix api",
    "dev": "concurrently \"npm run api\" \"npm start\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}